#include "s21_matrix.h"

#test s21_create_matrix_01 
  int res = 0;
  matrix_t A = {0};

  res = s21_create_matrix(1, 1, &A);
  ck_assert_int_eq(res, CONVERSATION_OK);
  s21_remove_matrix(&A);

